[core]
	filemode = false
[alias]
        st = status
        co = checkout
        alias = config --get-regexp alias
        cb = checkout -b
        aa = add -A
        cm = commit -m
        ca = commit -a -m
        m = checkout master
        lg = log --graph --pretty=format:'%Cred%h%Creset -%C(yellow)%d%Creset %s %Cgreen(%cr) %C(bold blue)<%an>%Creset' --abbrev-commit --date=relative
[color]
        ui = true
[heroku]
	account = personal
